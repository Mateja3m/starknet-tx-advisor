{
  "expo": {
    "name": "SMDAK Starter dApp",
    "slug": "smdak-dapp",
    "scheme": "smdak-dapp",
    "version": "0.1.0",
    "orientation": "portrait",
    "platforms": ["android", "ios"],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.24"
          }
        }
      ]
    ],
    "android": {
      "package": "com.smdak.dapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": false,
          "data": [{ "scheme": "smdak-dapp" }],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.smdak.dapp"
    }
  }
}
