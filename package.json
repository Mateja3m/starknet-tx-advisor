{
  "name": "starknet-mwa-poc",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "dapp",
    "apps/wallet",
    "packages/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "start:dapp": "npm --workspace dapp run start",
    "start:wallet": "npm --workspace apps/wallet run start",
    "android:dapp": "npm --workspace dapp run android",
    "android:wallet": "npm --workspace apps/wallet run android"
  }
}
